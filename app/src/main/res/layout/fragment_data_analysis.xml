<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.meteor.fragment.DataAnalysisFragment">

    <TextView
        android:id="@+id/tv_sum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:text="普通用户总数："
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ana_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:text="sum"
        app:layout_constraintStart_toEndOf="@+id/tv_sum"
        app:layout_constraintTop_toTopOf="@+id/tv_sum" />

    <TextView
        android:id="@+id/tv_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="普通用户总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_sum" />

    <TextView
        android:id="@+id/ana_user_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_1" />

    <TextView
        android:id="@+id/tv_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="女性用户总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_1" />

    <TextView
        android:id="@+id/ana_other_user_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_4" />

    <TextView
        android:id="@+id/tv_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="男性用户总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_2" />

    <TextView
        android:id="@+id/ana_male_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_3" />

    <TextView
        android:id="@+id/tv_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="未设置性别用户总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_3" />

    <TextView
        android:id="@+id/ana_asr_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_5" />

    <TextView
        android:id="@+id/tv_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="ASR使用总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_4" />

    <TextView
        android:id="@+id/ana_female_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_2" />


    <TextView
        android:id="@+id/tv_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="TTS使用总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_7" />

    <TextView
        android:id="@+id/ana_ocr_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_7" />

    <TextView
        android:id="@+id/tv_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="OCR使用总数："
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_5" />

    <TextView
        android:id="@+id/ana_tts_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_6" />

    <TextView
        android:id="@+id/tv_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="今日活跃用户量"
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_6" />

    <TextView
        android:id="@+id/tv_today_active_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_8" />

    <TextView
        android:id="@+id/tv_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="今日ASR使用总量"
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_8" />

    <TextView
        android:id="@+id/tv_today_asr_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_9" />

    <TextView
        android:id="@+id/tv_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="今日OCR使用总量"
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_9" />

    <TextView
        android:id="@+id/tv_today_ocr_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_10" />

    <TextView
        android:id="@+id/tv_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="今日TTS使用总量"
        app:layout_constraintStart_toStartOf="@+id/tv_sum"
        app:layout_constraintTop_toBottomOf="@+id/tv_10" />

    <TextView
        android:id="@+id/tv_today_tts_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sum"
        app:layout_constraintStart_toStartOf="@+id/ana_count"
        app:layout_constraintTop_toTopOf="@+id/tv_11" />
</androidx.constraintlayout.widget.ConstraintLayout>